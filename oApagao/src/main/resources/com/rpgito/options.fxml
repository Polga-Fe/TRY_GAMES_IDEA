<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Slider?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rpgito.optionsPage">
    <children>
        <Label fx:id="label" text="MENU DE OPÇÕES" textAlignment="CENTER" />
        <Slider fx:id="volumeSlider" min="0" max="100" value="50"/>
        <Button fx:id="monstos" text="MONSTRUARIO" textAlignment="CENTER" />
        <Button fx:id="outButton" onAction="#returnOpening" text="VOLTAR" textAlignment="CENTER" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
